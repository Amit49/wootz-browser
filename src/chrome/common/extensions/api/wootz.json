[
    {
      "namespace": "wootz",
      "description": "Wootz browser api",
      "compiler_options": {
        "implemented_in": "chrome/browser/extensions/api/wootz/wootz_api.h"
      },
      "types": [
        {
          "id": "JobResult",
          "type": "object",
          "description": "Result of URL monitoring job",
          "properties": {
            "url": {
              "type": "string",
              "description": "The monitored URL"
            },
            "response": {
              "type": "string",
              "description": "Response content from the URL"
            },
            "timestamp": {
              "type": "number",
              "description": "Timestamp of the fetch"
            }
          }
        },
        {
          "id": "ChainType",
          "type": "string",
          "enum": [
            "ethereum",
            "solana"
          ]
        }
      ],
      "functions": [
        {
          "name": "info",
          "type": "function",
          "description": "Gets wootz browser info",
          "parameters": [],
          "returns_async": {
            "name": "callback",
            "parameters": [
              {
                "type": "string",
                "name": "result"
              }
            ]
          }
        },
        {
          "name": "setSelectedChains",
          "type": "function",
          "description": "Sets the selected chains for wallet creation",
          "parameters": [
            {
              "name": "chains",
              "type": "array",
              "items": {
                "$ref": "ChainType"
              },
              "description": "Array of selected chains"
            }
          ],
          "returns_async": {
            "name": "callback",
            "parameters": [
              {
                "name": "result",
                "type": "object",
                "properties": {
                  "success": {
                    "type": "boolean"
                  }
                }
              }
            ]
          }
        },
        {
          "name": "createWallet",
          "type": "function",
          "description": "Creates a new wallet",
          "parameters": [
            {
              "name": "password",
              "type": "string",
              "description": "The password for the new wallet"
            }
          ],
          "returns_async": {
            "name": "callback",
            "parameters": [
              {
                "type": "object",
                "name": "result",
                "properties": {
                  "success": {"type": "boolean"},
                  "recoveryPhrase": {"type": "string", "optional": true},
                  "error": {"type": "string", "optional": true}
                }
              }
            ]
          }
        },
        {
          "name": "unlockWallet",
          "type": "function",
          "description": "Unlocks the wallet",
          "parameters": [
            {
              "name": "password",
              "type": "string",
              "description": "The wallet password"
            }
          ],
          "returns_async": {
            "name": "callback",
            "parameters": [
              {
                "type": "object",
                "name": "result",
                "properties": {
                  "success": {"type": "boolean"},
                  "error": {"type": "string", "optional": true}
                }
              }
            ]
          }
        },
        {
          "name": "lockWallet",
          "type": "function",
          "description": "Locks the wallet",
          "parameters": [],
          "returns": {
            "type": "object",
            "properties": {
              "success": {"type": "boolean"}
            }
          }
        },
        {
          "name": "isLocked",
          "type": "function",
          "description": "Checks if the wallet is locked",
          "parameters": [],
          "returns_async": {
            "name": "callback",
            "parameters": [
              {
                "type": "object",
                "name": "result",
                "properties": {
                  "isLocked": {"type": "boolean"},
                  "error": {"type": "string", "optional": true}
                }
              }
            ]
          }
        },
        {
          "name": "getAllAccounts",
          "type": "function",
          "description": "Gets all accounts in the wallet",
          "parameters": [],
          "returns_async": {
            "name": "callback",
            "parameters": [
              {
                "type": "object",
                "name": "result",
                "properties": {
                  "success": {"type": "boolean"},
                  "accounts": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "address": {"type":"string"},
                        "name": {"type": "string"},
                        "coin": {"type": "integer"}
                      }
                    }
                  },
                  "error": {"type": "string", "optional": true}
                }
              }
            ]
          }
        },
        {
          "name": "signMessage",
          "type": "function",
          "description": "Responds to a sign message request",
          "parameters": [
            {"name": "requestId", "type": "integer"},
            {"name": "approved", "type": "boolean"},
            {"name": "signature", "type": "string", "optional": true}
          ],
          "returns_async": {
            "name": "callback",
            "parameters": [
              {
                "type": "object",
                "name": "result",
                "properties": {
                  "success": {"type": "boolean"},
                  "error": {"type": "string", "optional": true}
                }
              }
            ]
          }
        },
        {
          "name": "helloWorld",
          "type": "function",
          "description": "Returns greeting message",
          "parameters": [],
          "returns_async": {
            "name": "callback",
            "parameters": [
              {
                "type": "string",
                "name": "result"
              }
            ]
          }
        },
        {
          "name": "showDialog",
          "type": "function",
          "description": "Shows the consent dialog and starts the service if consent is given",
          "parameters": []
        },
        {
          "name": "log",
          "type": "function",
          "description": "Log to browser debug logs",
          "parameters": [
            {
              "type": "string",
              "name": "log"
            }
          ],
          "returns_async": {
            "name": "callback",
            "parameters": []
          }
        },
        {
          "name": "setJob",
          "type": "function",
          "description": "Start monitoring a URL",
          "parameters": [
            {
              "name": "url",
              "type": "string",
              "description": "URL to monitor"
            }
          ]
        },
        {
          "name": "removeJob",
          "type": "function",
          "description": "Stop monitoring a URL",
          "parameters": [
            {
              "name": "url",
              "type": "string",
              "description": "URL to stop monitoring"
            }
          ]
        },
        {
          "name": "getJobs",
          "type": "function",
          "description": "Get monitoring results for a URL",
          "parameters": [],
          "returns_async": {
            "name": "callback",
            "parameters": [
              {
                "name": "results",
                "type": "array",
                "items": {
                  "$ref": "JobResult"
                },
                "description": "Array of job results"
              }
            ]
          }
        },
        {
          "name": "listJobs",
          "type": "function",
          "description": "List all monitored URLs",
          "parameters": [],
          "returns_async": {
            "name": "callback",
            "parameters": [
              {
                "name": "urls",
                "type": "array",
                "items": {
                  "type": "string"
                },
                "description": "Array of monitored URLs"
              }
            ]
          }
        },
        {
          "name": "cleanJobs",
          "type": "function",
          "description": "Clear all jobs and results",
          "parameters": []
        }
      ],
      "events": [
        {
          "name": "onSignMessageRequested",
          "type": "function",
          "description": "Fired when a new sign message request is available",
          "parameters": [
            {
              "name": "request",
              "type": "object",
              "properties": {
                "id": {"type": "integer"},
                "address": {"type": "string"},
                "origin": {"type": "string"},
                "chainId": {"type": "string"},
                "isEip712": {"type": "boolean"}
              }
            }
          ]
        }
      ]
    }
]
